{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "inputs": [
        {
            "id": "date",
            "description": "Solve puzzle from which date?",
            "type": "promptString",
            "default": "2022-05-27"
        }
    ],
    "configurations": [
        {
            "name": "Main",
            "args": [ "${input:date}.txt" ],
            "type": "python",
            "request": "launch",
            "program": "__main__.py",
            "console": "integratedTerminal",
            "justMyCode": true,
        }
    ]
}