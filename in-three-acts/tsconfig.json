{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "out",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2016",
  },
  "include": [ "src" ]
}
